{% extends 'base.html.twig' %}

{% block title %}Deposito{% endblock %}

{% block body %}
    <h1>Deposito</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ deposito.id }}</td>
            </tr>
            <tr>
                <th>Monto</th>
                <td>{{ deposito.monto }}</td>
            </tr>
            <tr>
                <th>Anio</th>
                <td>{{ deposito.anio }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ deposito.mes }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ deposito.observacion }}</td>
            </tr>
            <tr>
                <th>FechaDeposito</th>
                <td>{{ deposito.fechaDeposito ? deposito.fechaDeposito|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_deposito_index') }}">back to list</a>

    <a href="{{ path('app_deposito_edit', {'id': deposito.id}) }}">edit</a>

    {{ include('deposito/_delete_form.html.twig') }}
{% endblock %}
